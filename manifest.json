{
  "manifest_version": 2,

  "name": "TaiEngDic",
  "description": "This extension will provide English-Shan translations",
  "version": "0.5",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["main.css"],
      "js": ["main.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
